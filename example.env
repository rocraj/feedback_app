# Example .env file - DO NOT COMMIT REAL CREDENTIALS
# Copy this file to .env and fill in with your actual values

# Database Configuration
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_HOST=your_db_host
DB_PORT=5432
DB_NAME=your_db_name

# Or use a full connection string
# DATABASE_URL=postgresql://user:password@host:port/dbname

# Email Configuration
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your_email@example.com
SMTP_PASSWORD=your_email_password
EMAILS_FROM_EMAIL=noreply@example.com

# Security
SECRET_KEY=your_secret_key_should_be_long_and_random

# Frontend URLs
FRONTEND_URL=https://feedback-mini.web.app
CORS_ORIGINS=["https://feedback-mini.web.app", "http://localhost:5173"]

# API Configuration
API_BASE_URL=https://feedback-backend-app.uc.r.appspot.com/api/v1